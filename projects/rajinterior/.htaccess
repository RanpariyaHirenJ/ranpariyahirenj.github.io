ErrorDocument 403 https://www.rajinterior.co.in/404.html
ErrorDocument 404 https://www.rajinterior.co.in/404.html
ErrorDocument 500 https://www.rajinterior.co.in/404.html

Options -Indexes

Options +FollowSymLinks


RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d
RewriteRule ^(.*)$ ./route.php?url=$1 [L,QSA]

RewriteEngine On



RewriteCond %{SERVER_PORT} 80 
RewriteRule ^(.*)$ https://www.rajinterior.co.in/$1 [R,L]

RewriteCond %{HTTP_HOST} ^rajinterior.co.in [NC]
RewriteRule ^(.*)$ https://www.rajinterior.co.in/$1 [L,R=301]

