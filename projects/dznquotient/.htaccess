ErrorDocument 403 https://www.dznquotient.com/404.html
ErrorDocument 404 https://www.dznquotient.com/404.html
ErrorDocument 500 https://www.dznquotient.com/404.html

Options -Indexes

Options +FollowSymLinks


RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d
RewriteRule ^(.*)$ ./route.php?url=$1 [L,QSA]

RewriteEngine On



RewriteCond %{SERVER_PORT} 80 
RewriteRule ^(.*)$ https://www.dznquotient.com/$1 [R,L]

RewriteCond %{HTTP_HOST} ^dznquotient.com [NC]
RewriteRule ^(.*)$ https://www.dznquotient.com/$1 [L,R=301]

#RewriteCond %{HTTP_HOST} ^dznquotient\.com [NC]
#RewriteRule (.*) https://www.dznquotient.com/$1 [L,R=301]